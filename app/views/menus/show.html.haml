.row
  .col-md-3
    = render 'shared/admin_menu'
  .col-md-9
    %h3.page-header
      = @menu.name
      .pull-right= link_to '< Back', website_path(@website), class: 'btn btn-link'
      .pull-right= link_to 'Edit', edit_website_menu_path(@website), class: 'btn btn-link'

    .row
      .col-md-6.form-group
        Created at
        = @menu.created_at
